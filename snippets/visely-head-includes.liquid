<script type="text/javascript">
  var ViselyPage = '{{template.name}}';
  var ViselyResource = {theme:"{{theme.id}}",page:"{{template.name}}",template:"{{template.suffix}}",handle:"{{handle}}"};
  var ViselyProductId = '{{product.id}}';
  var ViselyProductType = '{{product.type}}';
  var ViselyProductTags = '{{ product.tags | join: ',' | escape}}';
  var ViselyArticleTags = '{{ article.tags | join: ',' | escape}}'; 
  var ViselyCartProductIds = '{{ cart.items | map: 'product_id' | join: ','}}';
  {%unless collectionIds%} 
    {% assign collectionIds = '' %}
    {% if product %}
      {% capture collectionIds %}{{ product.collections | map: 'id' | join: "," }}{% endcapture %}
    {% elsif collection %}
      {% capture collectionIds %}{{ collection.id }}{% endcapture %}  
    {% endif%}  
  {% endunless %}
  var ViselyCollectionIds = '{{collectionIds}}';
  var ViselyProductName = '{{product.title | escape}}';
  var ViselyArticleName = '{{ article.title  | escape | split: ":" | first}}';
  var ViselyPageLoadStart = ViselyPageLoadStart || performance ? performance.now() : -1;
  var ViselyContext = ViselyContext || {% if customer %}{ 'currentCustomer': { 'email': '{{customer.email}}' } }{% else %}{}{% endif %};  
</script>
